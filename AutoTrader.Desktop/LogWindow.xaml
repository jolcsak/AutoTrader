<Window x:Class="AutoTrader.Desktop.LogWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AutoTrader.Desktop"
        mc:Ignorable="d"
        Title="LogWindow" Height="651" Width="662" Closed="Window_Closed">
    <DockPanel LastChildFill="true" Margin="10,10,10,10" Grid.Row="0">
        <TextBox x:Name="console"  TextWrapping="Wrap" IsUndoEnabled="False" VerticalScrollBarVisibility="Visible" DockPanel.Dock="Top" AllowDrop="False" />
    </DockPanel>
</Window>
